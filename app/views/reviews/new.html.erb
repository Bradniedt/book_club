<%= form_for [@book, @review] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br/>
  <%= f.label :username %>
  <%= f.text_field :username, :value => "" %><br/>
  <%= f.label :rating %>
  <%= f.number_field :rating %><br/>
  <%= f.label :description %>
  <%= f.text_field :description %><br/>
  <%= f.submit %><br/>
<% end %>
